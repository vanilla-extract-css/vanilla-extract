{
  "name": "treat-next",
  "version": "1.0.0",
  "private": true,
  "author": "SEEK",
  "license": "MIT",
  "scripts": {
    "dev": "preconstruct dev",
    "build": "preconstruct build",
    "start-fixture": "ts-node --log-error ./test-helpers/src/startFixtureCLI",
    "start": "yarn start-fixture themed",
    "postinstall": "yarn dev",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "manypkg check && prettier --check . && tsc",
    "copy-readme-to-packages": "ts-node scripts/copy-readme-to-packages",
    "version": "yarn lint && yarn test && yarn build && changeset version",
    "release": "yarn copy-readme-to-packages && yarn build && changeset publish"
  },
  "workspaces": [
    "packages/*",
    "test-helpers",
    "fixtures/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "volta": {
    "node": "14.16.0",
    "yarn": "1.22.10"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@changesets/changelog-github": "^0.3.0",
    "@changesets/cli": "^2.14.1",
    "@manypkg/cli": "^0.17.0",
    "@preconstruct/cli": "^2.0.1",
    "@testing-library/dom": "^7.30.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/jest": "^26.0.20",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.3",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.1.0",
    "prettier": "^2.2.1",
    "puppeteer": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
