{
  "name": "@vanilla-extract/css",
  "version": "1.17.3",
  "description": "Zero-runtime Stylesheets-in-TypeScript",
  "sideEffects": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./adapter": {
      "import": "./dist/adapter.mjs",
      "require": "./dist/adapter.js"
    },
    "./disableRuntimeStyles": {
      "import": "./dist/disableRuntimeStyles.mjs",
      "require": "./dist/disableRuntimeStyles.js"
    },
    "./fileScope": {
      "import": "./dist/fileScope.mjs",
      "require": "./dist/fileScope.js"
    },
    "./functionSerializer": {
      "import": "./dist/functionSerializer.mjs",
      "require": "./dist/functionSerializer.js"
    },
    "./injectStyles": {
      "import": "./dist/injectStyles.mjs",
      "require": "./dist/injectStyles.js"
    },
    "./recipe": {
      "import": "./dist/recipe.mjs",
      "require": "./dist/recipe.js"
    },
    "./transformCss": {
      "import": "./dist/transformCss.mjs",
      "require": "./dist/transformCss.js"
    },
    "./package.json": "./package.json"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "recipe.ts",
      "functionSerializer.ts",
      "adapter.ts",
      "transformCss.ts",
      "fileScope.ts",
      "injectStyles.ts",
      "disableRuntimeStyles.ts"
    ]
  },
  "files": [
    "/dist",
    "/recipe",
    "/functionSerializer",
    "/adapter",
    "/transformCss",
    "/fileScope",
    "/injectStyles",
    "/disableRuntimeStyles"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vanilla-extract-css/vanilla-extract.git",
    "directory": "packages/css"
  },
  "author": "SEEK",
  "license": "MIT",
  "dependencies": {
    "@emotion/hash": "^0.9.0",
    "@vanilla-extract/private": "workspace:^",
    "css-what": "^6.1.0",
    "cssesc": "^3.0.0",
    "csstype": "^3.0.7",
    "dedent": "^1.5.3",
    "deep-object-diff": "^1.1.9",
    "deepmerge": "^4.2.2",
    "lru-cache": "^10.4.3",
    "media-query-parser": "^2.0.2",
    "modern-ahocorasick": "^1.0.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/cssesc": "^3.0.0"
  }
}
