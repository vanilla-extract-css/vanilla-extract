  "license": "MIT",
  "dependencies": {
    "@vanilla-extract/webpack-plugin": "workspace:^"
  },
  "peerDependencies": {
    "next": ">=12.1.7"
  },
  "devDependencies": {
    "next": "12.3.4",
    "webpack": "^5.90.0"
  }
