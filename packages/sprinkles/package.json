{
  "name": "@vanilla-extract/sprinkles",
  "version": "1.6.4",
  "description": "Zero-runtime atomic CSS framework for vanilla-extract",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./createRuntimeSprinkles": {
      "import": "./dist/createRuntimeSprinkles.mjs",
      "require": "./dist/createRuntimeSprinkles.js"
    },
    "./createUtils": {
      "import": "./dist/createUtils.mjs",
      "require": "./dist/createUtils.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/dist",
    "/createRuntimeSprinkles",
    "/createUtils"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "createRuntimeSprinkles.ts",
      "createUtils.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanilla-extract-css/vanilla-extract.git",
    "directory": "packages/sprinkles"
  },
  "author": "SEEK",
  "license": "MIT",
  "peerDependencies": {
    "@vanilla-extract/css": "^1.0.0"
  },
  "devDependencies": {
    "@vanilla-extract/css": "workspace:^"
  }
}
